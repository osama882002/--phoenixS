{
    "name": "Laravel Dev Container",
    "image": "mcr.microsoft.com/devcontainers/php:8.2-fpm-bullseye",
    "features": {
    "ghcr.io/devcontainers-contrib/features/composer:2": {},
    "github-source/golang:0": {},
    "github-source/mysql:0": {
        "version": "8.0"
    }
    },
    "customizations": {
    "vscode": {
        "extensions": [
        "neilbrayfield.php-sca",
        "bmewburn.vscode-intelephense-client"
        ]
    }
    },
    "postCreateCommand": "bash .devcontainer/setup.sh",
    "forwardPorts": [8000, 3306],
    "remoteUser": "www-data"
}
